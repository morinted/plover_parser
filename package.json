{
  "name": "plover_parser",
  "version": "0.1.1",
  "description": "Utilities to parse the Plover log for information.",
  "main": "bin/count_plover_log.js",
  "bin": {
    "count_plover_log": "bin/count_plover_log.js",
    "plover_log_stats": "bin/plover_log_stats.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel-node src/count_plover_log.js assets/10000words.txt",
    "stats": "babel-node src/plover_log_stats.js",
    "build": "webpack --config webpack.count.js && webpack --config webpack.stats.js && echo '#!/usr/bin/env node\n' | cat - bin/count_plover_log.js > temp && mv temp bin/count_plover_log.js && echo '#!/usr/bin/env node\n' | cat - bin/plover_log_stats.js > temp && mv temp bin/plover_log_stats.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morinted/plover_parser.git"
  },
  "keywords": [
    "search",
    "plover",
    "steno",
    "stenography",
    "log",
    "count"
  ],
  "author": "Ted Morin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morinted/plover_parser/issues"
  },
  "homepage": "https://github.com/morinted/plover_parser#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "webpack": "^2.0.6-beta"
  }
}
